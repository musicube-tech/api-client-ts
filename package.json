{
  "name": "@musicube/api-client",
  "version": "0.0.4",
  "description": "client for musicube api",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:ts": "tsc",
    "build:set-version": "sed -i.bak \"s/VERSION/$npm_package_version/\" dist/config/index.js && rm dist/config/index.js.bak",
    "build:factories": "create-typed-factories --extra-tags faker,type --no-type-check --extend-file src/config/extendTypedFactories.ts --output-file src/factories.ts src/types.ts",
    "cleanup": "echo \"node_modules\" > .gitignore && git clean -df && git co .gitignore",
    "prepare": "npm run cleanup && npm run build:factories && npm run build:ts && npm run build:set-version && cp -r dist/* ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/musicube-tech/api-client-ts.git"
  },
  "keywords": [
    "client",
    "musicube",
    "music",
    "rich",
    "music",
    "data"
  ],
  "author": "Hannes Diercks <hdiercks@musicu.be> (https://www.musicu.be/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/musicube-tech/api-client-ts/issues"
  },
  "homepage": "https://github.com/musicube-tech/api-client-ts#readme",
  "devDependencies": {
    "@types/faker": "5.5.8",
    "@types/json-schema-faker": "0.5.1",
    "@types/node": "16.7.8",
    "date-fns": "2.23.0",
    "faker": "5.5.3",
    "json-schema-faker": "*",
    "type-fest": "2.1.0",
    "typed-factories": "0.1.0"
  },
  "optionalDependencies": {
    "date-fns": "^2.23.0",
    "faker": "^5.5.3",
    "json-schema-faker": "^0.4 || ^0.5"
  }
}
